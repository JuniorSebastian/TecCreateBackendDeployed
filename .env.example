# Copy this file to .env and fill values for your environment

# App
PORT=8080
NODE_ENV=development
CLIENT_URL=http://localhost:3000

# Database (example using DATABASE_URL)
# Postgres connection string: postgres://user:pass@host:5432/dbname
DATABASE_URL=

# If using TLS with a CA, set the base64 PEM here
# DATABASE_SSL_CA_B64=

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:8080/auth/google/callback

# JWT
JWT_SECRET=changeme

# Session
SESSION_SECRET=changeme
